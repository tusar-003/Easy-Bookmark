{
  "manifest_version": 3,
  "name": "Easy Bookmarks",
  "version": "1.2",
  "description": "Display all bookmarks as icons on new tab page",
  "permissions": ["bookmarks", "storage", "topSites", "history", "favicon"],
  "background": {
    "service_worker": "background.js"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}