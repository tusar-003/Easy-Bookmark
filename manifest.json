{
    "manifest_version": 3,
    "name": "Compact Bookmark Icons",
    "version": "1.0",
    "description": "Display all bookmarks as icons on new tab page",
    "permissions": ["bookmarks", "storage", "topSites"],
    "chrome_url_overrides": {
      "newtab": "newtab.html"
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }